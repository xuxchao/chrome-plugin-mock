{
  "manifest_version": 3,
  "name": "请求重定向测试",
  "version": "1.0",
  "permissions": ["declarativeNetRequest"],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "src/background.js"
  },
  "action": {
    "default_popup": "popup/popup.html",
    "default_title": "DNR 规则管理",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
}